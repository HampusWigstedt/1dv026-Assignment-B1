<div class="container">
  <!-- Example row of columns -->
  <div id="box">
    <h1 class="display-3">
      CRUD
    </h1>
    <p>
      The crud application
    </p>
    <form action="/register" method="post">
      <div class="form-element">
        <label for="username">Username:</label>
